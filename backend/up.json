{
    "name": "beauty-partner",
    "profile": "beautyuser", 
    "regions": ["eu-west-1"],
    "lambda": {
      "memory": 512,
      "runtime": "nodejs8.10"
    },
    "proxy": {
      "command": "npm start",
      "timeout": 25,
      "listen_timeout": 15,
      "shutdown_timeout": 15
    },
    "stages": {
      "development": {
        "proxy": {
          "command": "yarn dev"
        }
      }
    },
    "environment": { 
      "NODE_ENV": "dev",
      "PRISMA_STAGE": "dev",
      "PRISMA_ENDPOINT": "https://eu1.prisma.sh/beauty-parner/main/dev",
      "PRISMA_SECRET": "HYBEARPWD0803",
      "APP_SECRET": "JWTSECRET888"
    },
    "error_pages": { 
      "variables": {
        "support_email": "castro.patrick@outlook.com",
        "color": "#F57B51"
      }
    },
    "cors": { 
      "allowed_origins": ["http://localhost:3000"],
      "allowed_methods": ["HEAD", "GET", "POST"],
      "allowed_headers": ["*"]
    }
  }